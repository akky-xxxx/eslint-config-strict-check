{
  "baseBranches": ["develop"],
  "extends": ["config:base"],
  "labels": ["01.greenkeeping"],
  "lockFileMaintenance": {
    "enabled": true
  },
  "packageRules": [
    {
      "automerge": true,
      "excludePackageNames": ["typescript"],
      "groupName": "dependencies (non-major)",
      "matchUpdateTypes": ["minor", "patch"],
      "rangeStrategy": "pin"
    }
  ],
  "vulnerabilityAlerts": {
    "enabled": true
  }
}
